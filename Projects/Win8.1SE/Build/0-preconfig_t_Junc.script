[Process]
If,%OSFamily%,Equal,NT6,Begin
//To avoid DirMove FallBack that cause too much time at lost config: Junc Target and TargetImage at the same time.
If,%Target_CheckBox%,Equal,True,Set,%TargetImage_CheckBox%,True,Permanent
If,%TargetImage_CheckBox%,Equal,True,Set,%Target_CheckBox%,True,Permanent
End
//-
If,%ProjectName%,Equal,Gena,IniRead,%ScriptFile%,Main,WebRootFile_Gena,%t%
If,%ProjectName%,Equal,Gena,StrFormat,FileName,%ScriptFile%,%ScriptFileName%
Set,%Gena_Exit%,0
If,%ProjectName%,Equal,Gena,Begin
If,Not,%ScriptFileName%,Equal,%t%,Set,%Gena_Exit%,1
End
If,%Gena_Exit%,Equal,1,Exit,"Gena use Junction Plugin at Basic\Build\'Junction' - Exiting"
